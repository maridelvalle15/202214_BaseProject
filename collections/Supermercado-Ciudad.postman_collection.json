	"item": [
		{
			"name": "Asociar un nuevo supermercado a una ciudad",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"//crear ciudad",
							"var optionsCiudad = {",
							"  url:  pm.variables.get(\"baseUrl\")+\"/ciudades\",",
							"  method: 'POST',",
							"  header: {",
							"    'Accept': '*/*',",
							"    'Content-Type': 'application/json',",
							"  },",
							"  body: {",
							"    mode: 'raw',",
							"    raw : JSON.stringify({",
							"        \"nombre\": \"Ciudad 1\",",
							"        \"pais\": \"Argentina\",",
							"        \"num_habitantes\": 1500",
							"    })",
							"  }",
							"};",
							"",
							"const interval = setTimeout(() => {}, 4000);",
							"",
							"// First request we're making",
							"pm.sendRequest(optionsCiudad, (err, res) => {",
							"    if (err) {",
							"        console.log(\"Oh no. Couldn't complete the 1st request: \", err);",
							"        clearTimeout(interval);",
							"    } else {",
							"        console.log(\"Yay! Completed the 1st request\");",
							"",
							"        var dataCiudad = res.json()",
							"        pm.globals.set(\"ciudad_id\", dataCiudad.id);",
							"",
							"        var optionsSupermercado = {",
							"            url:  pm.variables.get(\"baseUrl\")+\"/supermercados\",",
							"            method: 'POST',",
							"            header: {",
							"                'Accept': '*/*',",
							"                'Content-Type': 'application/json',",
							"            },",
							"            body: {",
							"                mode: 'raw',",
							"                raw : JSON.stringify({",
							"                    \"nombre\": \"Supermercado 1\",",
							"                    \"longitud\": \"74°04'51''\",",
							"                    \"latitud\": \"4° 35'56''\",",
							"                    \"pag_web\": \"pag-web-supermercado.com.co\"",
							"                })",
							"            }",
							"        };",
							"",
							"        pm.sendRequest(optionsSupermercado, function (err, res) {",
							"            if (err) {",
							"                console.log(\"Oh no. Couldn't complete the 2st request: \", err);",
							"                clearTimeout(interval);",
							"            } else {",
							"                console.log('Yay! Completed the 2nd request');",
							"",
							"                var dataSupermercado = res.json()",
							"                pm.globals.set(\"supermercado_id\", dataSupermercado.id);",
							"            }",
							"        });",
							"        clearTimeout(interval);",
							"    }",
							"});"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {",
							"   pm.response.to.have.status(201);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/ciudades/{{ciudad_id}}/supermercados/{{supermercado_id}}",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"ciudades",
						"{{ciudad_id}}",
						"supermercados",
						"{{supermercado_id}}"
					]
				}
			},
			"response": []
